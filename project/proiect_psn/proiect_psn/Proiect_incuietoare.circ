		reset : in std_logic;
		master_reset : in std_logic;
		en_up : out std_logic;
		en_down : out std_logic;
		mem_cif1 : out std_logic;
		mem_cif2 : out std_logic;
		mem_cif3 : out std_logic;
		mem_cif4 : out std_logic;
		mem_cif5 : out std_logic;
		mem_cif6 : out std_logic;
		RC : out std_logic;
		r_tot_reg : out std_logic;
		r_part_reg : out std_logic);
END UC;

ARCHITECTURE TypeArchitecture OF UC IS

type mod_stare is (ST0, ST1, ST2, ST3, ST4, ST5, ST6, ST7, ST8, ST9, ST10, ST11, ST12, ST13, ST14, ST15);

signal stare, nxstare : mod_stare;

signal verod_cif : std_logic := '0';

BEGIN

	act_stare: process(clk)
	begin
		if clk'event and clk = '1' then
			stare &lt;= nxstare;
		end if;
	end process act_stare;

	trans_stare: process(stare, clk)
	begin

	en_up &lt;= '0';
	en_down &lt;= '0';

	if clk = '1' then
		intr_cif &lt;= '0';
		liber_ocupat &lt;= '0';
		mem_cif1 &lt;= '0';
		mem_cif2 &lt;= '0';
		mem_cif3 &lt;= '0';
		mem_cif4 &lt;= '0';
		mem_cif5 &lt;= '0';
		mem_cif6 &lt;= '0';
		RC &lt;= '0';
		r_tot_reg &lt;= '0';
		r_part_reg &lt;= '0';
		
		case stare is
			when ST0 =&gt;
				if start = '1' then
					nxstare &lt;= ST1;
				end if;
			when ST1 =&gt;
				if ad_cif = '1' then
					nxstare &lt;= ST2;
				end if;
			when ST2 =&gt;							--cif1
				intr_cif &lt;= '1';
				
				if ad_cif = '0' then verod_cif &lt;= '1';
				end if;

				if ad_cif = '1' and verod_cif = '1' then
					mem_cif1 &lt;= '1';	RC &lt;= '1';	verod_cif &lt;= '0';	nxstare &lt;= ST4;
				end if;
				if up = '1' then
					en_up &lt;= '1';
					nxstare &lt;= ST3;
				elsif down = '1' then
					en_down &lt;= '1';
					nxstare &lt;= ST3;
				end if;
			when ST3 =&gt;
				intr_cif &lt;= '1';
				if up = '0' and down = '0' then
				
					if ad_cif = '1' then
						mem_cif1 &lt;= '1';	RC &lt;= '1';
						nxstare &lt;= ST4;
					else
						nxstare &lt;= ST2;
					end if;
				end if;
			when ST4 =&gt;						--cif2
				intr_cif &lt;= '1';
				if ad_cif = '0' then verod_cif &lt;= '1';
				end if;

				if ad_cif = '1' and verod_cif = '1' then
					mem_cif2 &lt;= '1';	RC &lt;= '1';	verod_cif &lt;= '0';	nxstare &lt;= ST6;
				end if;				
				if up = '1' then
					en_up &lt;= '1';
					nxstare &lt;= ST5;
				elsif down = '1' then
					en_down &lt;= '1';
					nxstare &lt;= ST5;
				end if;
			when ST5 =&gt;
				intr_cif &lt;= '1';
				if up = '0' and down = '0' then
					if ad_cif = '1' then
						mem_cif2 &lt;= '1';	RC &lt;= '1';
						nxstare &lt;= ST6;
					else
						nxstare &lt;= ST4;
					end if;
				end if;
			when ST6 =&gt;						--cif3
				intr_cif &lt;= '1';
				if ad_cif = '0' then verod_cif &lt;= '1';
				end if;

				if ad_cif = '1' and verod_cif = '1' then
					mem_cif3 &lt;= '1';	RC &lt;= '1';	verod_cif &lt;= '0';	nxstare &lt;= ST8;
				end if;
				if up = '1' then
					en_up &lt;= '1';
					nxstare &lt;= ST7;
				elsif down = '1' then
					en_down &lt;= '1';
					nxstare &lt;= ST7;
				end if;
			when ST7 =&gt;
				intr_cif &lt;= '1';
				if up = '0' and down = '0' then
					if ad_cif = '1' then
						mem_cif3 &lt;= '1';	RC &lt;= '1';
						nxstare &lt;= ST8;
					else
						nxstare &lt;= ST6;
					end if;
				end if;
			when ST8 =&gt;							--blocat
				liber_ocupat &lt;= '1';
				if ad_cif = '0' then verod_cif &lt;= '1';
				end if;
				
				if ad_cif = '1' and verod_cif = '1' then
					nxstare &lt;= ST9;
				end if;
			when ST9 =&gt;							--cif1vf
				liber_ocupat &lt;= '1';
				intr_cif &lt;= '1';
				if ad_cif = '0' then verod_cif &lt;= '1';
				end if;

				if ad_cif = '1' and verod_cif = '1' then
					mem_cif4 &lt;= '1';	RC &lt;= '1';	verod_cif &lt;= '0';	nxstare &lt;= ST11;
				end if;
				
				if up = '1' then
					en_up &lt;= '1';
					nxstare &lt;= ST10;
				elsif down = '1' then
					en_down &lt;= '1';
					nxstare &lt;= ST10;
				end if;
			when ST10 =&gt;
				liber_ocupat &lt;= '1';
				intr_cif &lt;= '1';
				
				if up = '0' and down = '0' then
					if ad_cif = '1' then
						mem_cif4 &lt;= '1';	RC &lt;= '1';
						nxstare &lt;= ST11;
					else
						nxstare &lt;= ST9;
					end if;
				end if;
			when ST11 =&gt;							--cif2vf
				liber_ocupat &lt;= '1';
				intr_cif &lt;= '1';
				if ad_cif = '0' then verod_cif &lt;= '1';
				end if;

				if ad_cif = '1' and verod_cif = '1' then
					mem_cif5 &lt;= '1';	RC &lt;= '1';	verod_cif &lt;= '0';	nxstare &lt;= ST13;
				end if;
				
				if up = '1' then
					en_up &lt;= '1';
					nxstare &lt;= ST12;
				elsif down = '1' then
					en_down &lt;= '1';
					nxstare &lt;= ST12;
				end if;
			when ST12 =&gt;
				liber_ocupat &lt;= '1';
				intr_cif &lt;= '1';
				
				if up = '0' and down = '0' then
					if ad_cif = '1' then
						mem_cif5 &lt;= '1';	RC &lt;= '1';
						nxstare &lt;= ST13;
					else
						nxstare &lt;= ST11;
					end if;
				end if;
			when ST13 =&gt;							--cif3vf
				liber_ocupat &lt;= '1';
				intr_cif &lt;= '1';
				if ad_cif = '0' then verod_cif &lt;= '1';
				end if;

				if ad_cif = '1' and verod_cif = '1' then
					mem_cif6 &lt;= '1';	RC &lt;= '1';	verod_cif &lt;= '0';	nxstare &lt;= ST15;
				end if;
				
				if up = '1' then
					en_up &lt;= '1';
					nxstare &lt;= ST14;
				elsif down = '1' then
					en_down &lt;= '1';
					nxstare &lt;= ST14;
				end if;
			when ST14 =&gt;
				liber_ocupat &lt;= '1';
				intr_cif &lt;= '1';
				
				if up = '0' and down = '0' then
					if ad_cif = '1' then
						mem_cif6 &lt;= '1';	RC &lt;= '1';
						nxstare &lt;= ST15;
					else
						nxstare &lt;= ST13;
					end if;
				end if;
			when ST15 =&gt;								--vf deblocare/ revenire in starea initiala
				liber_ocupat &lt;= '1';
				if egal1 = '1' and egal2 = '1' and egal3 = '1' then
					nxstare &lt;= ST1;	r_tot_reg &lt;= '1';	r_part_reg &lt;= '1';
				else
					nxstare &lt;= ST8;	r_part_reg &lt;= '1';
				end if;
		end case;


	end if;
	end process trans_stare;

END TypeArchitecture;
</vhdl>
  <vhdl name="SSD_DEC">LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY SSD_DEC IS
   PORT(nr : in std_logic_vector(3 downto 0);
		anod : in std_logic_vector(7 downto 0);
		mem6, mem5, mem3, mem2 : in std_logic;
		lib_ocup : in std_logic;
		q : out std_logic_vector(6 downto 0);
		on_off : in std_logic;
		reset : in std_logic;
		master_reset : in std_logic);			
END SSD_DEC;

ARCHITECTURE TypeArchitecture OF SSD_DEC IS

signal s2, s3, s5, s6 : std_logic := '0';

BEGIN

	process(anod)

	variable aux : std_logic_vector(6 downto 0) := (others =&gt; '0');
	
	begin
		if mem2 = '1' then s2 &lt;= '1';
		end if;
		if mem3 = '1' then s3 &lt;= '1';
		end if;
		if mem5 = '1' then s5 &lt;= '1';
		end if;
		if mem6 = '1' then s6 &lt;= '1';
		end if;
	
		case nr is
			when x"0" =&gt;  aux := "1111111";
   	 		when x"1" =&gt;  aux := "0110000";
    			when x"2" =&gt;  aux := "1101101";
    			when x"3" =&gt;  aux := "1111001";
    			when x"4" =&gt;  aux := "0110011";
    			when x"5" =&gt;  aux := "1011011";
    			when x"6" =&gt;  aux := "1011111";
    			when x"7" =&gt;  aux := "1110000";
    			when x"8" =&gt;  aux := "1111111";
    			when x"9" =&gt;  aux := "1111011";
    			when x"A" =&gt;  aux := "1110111";
    			when x"B" =&gt;  aux := "0011111";
    			when x"C" =&gt;  aux := "1001110";
    			when x"D" =&gt;  aux := "0111101";
    			when x"E" =&gt;  aux := "1001111";
    			when others =&gt; aux := "1000111";
		end case;

		case anod is
			when "11111110" =&gt;
			
			when "11111101" =&gt;
				if (s2 = '0' and lib_ocup = '0') or (s5 = '0' and lib_ocup = '1') then aux := "0000000";
				end if;
			when "11111011" =&gt;
				if (s3 = '0' and lib_ocup = '0') or (s6 = '0' and lib_ocup = '1') then aux := "0000000";
				end if;
			when others =&gt;	aux := "0000000";
		end case;

		if on_off = '0' then aux := "0000000";
		end if;

		if reset = '1' or master_reset = '1' then
			s2 &lt;= '0';	s3 &lt;= '0';	s5 &lt;= '0';	s6 &lt;= '0';
		elsif lib_ocup = '1' then 
			s2 &lt;= '0';	s3 &lt;= '0';
		else
			s5 &lt;= '0';	s6 &lt;= '0';
		end if;
		
		q &lt;= not aux;
	end process;
	
END TypeArchitecture;
</vhdl>
  <vhdl name="num_ssd">LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.std_logic_unsigned.all;


ENTITY num_ssd IS
	PORT(clk : in std_logic;
		num : out std_logic_vector(2 downto 0));
END num_ssd;

ARCHITECTURE TypeArchitecture OF num_ssd IS

signal aux : std_logic_vector(15 downto 0) := (others =&gt; '0');

BEGIN

	process(clk)
	begin
		if clk = '1' then
			num(2) &lt;= aux(2);
			num(1) &lt;= aux(1);
			num(0) &lt;= aux(0);
			aux &lt;= aux + 1;
		end if;
	end process;

END TypeArchitecture;
</vhdl>
  <vhdl name="catod_anod">LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.std_logic_unsigned.all
;

ENTITY catod_anod IS
	PORT(s : in std_logic_vector(2 downto 0);
		clk : in std_logic;
		liber_ocupat : in std_logic;
		d0, d1, d2, d3, d4, d5, q : in std_logic_vector(3 downto 0);
		catod : out std_logic_vector(3 downto 0);
		anod : out std_logic_vector(7 downto 0));
END catod_anod;

ARCHITECTURE TypeArchitecture OF catod_anod IS

signal aux : std_logic_vector(7 downto 0);

BEGIN
	process(clk)
	begin
		case s is
			when "000" =&gt;	
				if liber_ocupat = '0' then
					if d0 /= x"0" then catod &lt;= d0;
					else catod &lt;= q;	end if;
				end if;
				if liber_ocupat = '1' then
					if d3 /= x"0" then catod &lt;= d3;
					else catod &lt;= q;	end if;
				end if;
				aux &lt;= "00000001";
			when "001" =&gt;		
				if liber_ocupat = '0' then
					if d1 /= x"0" or d0 = x"0" then catod &lt;= d1;
					else catod &lt;= q;	end if;
				end if;
				if liber_ocupat = '1' then
					if d4 /= x"0" or d3 = x"0" then catod &lt;= d4;
					else catod &lt;= q;	end if;
				end if;
				aux &lt;= "00000010";
			when "010" =&gt;	
				if liber_ocupat = '0' then
					if d2 /= x"0" or d1 = x"0" then catod &lt;= d2;
					else catod &lt;= q;	end if;
				end if;
				if liber_ocupat = '1' then
					if d5 /= x"0" or d4 = x"0" then catod &lt;= d5;
					else catod &lt;= q;	end if;
				end if;
				aux &lt;= "00000100";
			when "011" =&gt; 
				catod &lt;= x"0";	aux &lt;= "00001000";
			when "100" =&gt; 
				catod &lt;= x"0";	aux &lt;= "00010000";
			when "101" =&gt; 
				catod &lt;= x"0";	aux &lt;= "00100000";
			when "110" =&gt; 
				catod &lt;= x"0";	aux &lt;= "01000000";
			when "111" =&gt;
				catod &lt;= x"0";	aux &lt;= "10000000";
			when others =&gt;
		end case;
		
		anod &lt;= not aux;
	end process;

END TypeArchitecture;
</vhdl>
