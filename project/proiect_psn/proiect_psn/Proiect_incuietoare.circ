ENTITY Comparator_4b IS
	PORT(a : in std_logic_vector(3 downto 0);
		b : in std_logic_vector(3 downto 0);
		egal : out std_logic);
END Comparator_4b;

ARCHITECTURE TypeArchitecture OF Comparator_4b IS

BEGIN

	process(a, b)
	begin
		if a = b then egal &lt;= '1';
